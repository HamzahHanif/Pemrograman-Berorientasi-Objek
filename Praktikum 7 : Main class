package hanip.praktikum7;

/**
 *
 * @author Praktikan
 */
public class HanipPraktikum7 {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here
        Mahasiswa mhs = new Mahasiswa("Hamzah Hanif","2117051032",90,80,90);
        
        mhs.printData("Ilmu Komputer");
        int hasil = mhs.sumValue(5, 8);
        System.out.println(hasil);
        
        mhs.hitungNA(90,90,80);
        
        //ambil nama 
        System.out.println("Nama sebelum diubah\t: " + mhs.getNama());
        
        //timpa nama make set
        mhs.setNama("Kevin");
        System.out.println("Nama setelah diubah\t: " + mhs.getNama());
    }
    
    
    
}
